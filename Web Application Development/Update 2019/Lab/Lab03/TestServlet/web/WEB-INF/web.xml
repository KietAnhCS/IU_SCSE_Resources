<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <welcome-file-list>
        <welcome-file>ThreeParams.jsp</welcome-file>
        <welcome-file>PersonalInfor.jsp</welcome-file>
        <welcome-file>RegisterForm.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>threeParam</servlet-name>
        <servlet-class>GetThreeParam</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>threeParam</servlet-name>
        <url-pattern>/ThreeParams/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>getInfor</servlet-name>
        <servlet-class>GetInfor</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>getInfor</servlet-name>
        <url-pattern>/getInfor/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ShowRegisterForm</servlet-name>
        <servlet-class>RegisterCourse</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ShowRegisterForm</servlet-name>
        <url-pattern>/RegisterCourse/</url-pattern>
    </servlet-mapping>

</web-app>