<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>LCookie</servlet-name>
        <servlet-class>LCookie</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LHidden</servlet-name>
        <servlet-class>LHidden</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LUrl</servlet-name>
        <servlet-class>LUrl</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LHttpObject</servlet-name>
        <servlet-class>LHttpObject</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LCookie</servlet-name>
        <url-pattern>/LCookie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LHidden</servlet-name>
        <url-pattern>/LHidden</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LUrl</servlet-name>
        <url-pattern>/LUrl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LHttpObject</servlet-name>
        <url-pattern>/LHttpObject</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
